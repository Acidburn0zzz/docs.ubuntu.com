#!/usr/bin/env bash
documentation-builder \
    --repository git@github.com:CanonicalLtd/ubuntu-core-docs.git \
    --branch master \
    --media-url /static/media/core \
    --output-media-path static/media/core \
    --template-path config/wrapper.jinja2 \
    --output-path templates/core \
    --no-link-extensions
